{
    "name": "raidhub",
    "version": "2023.7.17.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev+": "node proxy/server.js",
        "deploy": "./vercel/dev-deploy.sh",
        "db:start": "docker-compose -f docker-compose.yml up -d",
        "db:seed": "prisma db seed",
        "db:update": "prisma db push",
        "db:stop": "docker-compose -f docker-compose.yml down",
        "start": "next start",
        "lint": "next lint",
        "test": "jest",
        "format": "prettier --write \"src/**/*.{ts,tsx,json}\" && prisma format",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,json}\"",
        "sign": "cd proxy ; mkcert localhost",
        "build": "./vercel/build.sh"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^1.0.1",
        "@discordjs/collection": "^1.5.1",
        "@hookform/resolvers": "^3.1.1",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.0.0",
        "aws-sdk": "^2.1417.0",
        "bungie-net-core": "^1.1.7",
        "eslint-plugin-unused-imports": "^2.0.0",
        "formidable": "^3.5.0",
        "framer-motion": "^10.13.0",
        "next": "^13.1.6",
        "next-auth": "^4.22.1",
        "prisma": "^5.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.45.2",
        "sharp": "^0.32.2",
        "uuid": "^9.0.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@jest/globals": "^29.4.2",
        "@types/formidable": "^3.4.0",
        "@types/node": "18.0.0",
        "@types/react": "^18.0.33",
        "@types/react-dom": "18.0.5",
        "@types/uuid": "^9.0.2",
        "dotenv": "^16.0.3",
        "eslint": "8.40.0",
        "eslint-config-next": "13.4.2",
        "jest": "^29.5.0",
        "node-fetch": "^3.3.1",
        "prettier": "2.8.7",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "5.0"
    }
}
