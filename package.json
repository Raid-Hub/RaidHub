{
    "name": "raidhub",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev+": "node proxy/server.js",
        "manifest": "curl -o manifest.json https://api.raidhub.app/manifest -H 'origin: https://raidhub.io'",
        "docs": "openapi-typescript https://api-docs.raidhub.io/openapi.json -o ./src/types/raidhub-openapi.d.ts --immutable-types",
        "db:start": "docker-compose -f docker-compose.yml up -d",
        "db:schema": "prisma format && prisma db push",
        "db:push": "prisma format && prisma db push --force-reset",
        "db:stop": "docker-compose -f docker-compose.yml down",
        "studio": "prisma studio",
        "analyze": "ANALYZE=true yarn build",
        "lint": "next lint",
        "test": "jest",
        "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\" && prisma format",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
        "sign": "./proxy/sign.sh",
        "build": "./vercel/build.sh",
        "deploy": "./vercel/dev-deploy.sh"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.490",
        "@aws-sdk/s3-presigned-post": "^3.490",
        "@discordjs/collection": "^2.0.0",
        "@formatjs/intl-localematcher": "^0.5.4",
        "@hookform/resolvers": "^3.3.4",
        "@planetscale/database": "^1.14.0",
        "@prisma/adapter-planetscale": "5.9.1",
        "@prisma/client": "5.9.1",
        "@tanstack/react-query": "^4.36",
        "@trpc/client": "^10.44.1",
        "@trpc/next": "^10.44.1",
        "@trpc/react-query": "^10.44.1",
        "@trpc/server": "^10.44.1",
        "bungie-net-core": "latest",
        "dexie": "3.2.4",
        "dexie-react-hooks": "^1.1.7",
        "framer-motion": "^10.13.0",
        "html2canvas": "^1.4.1",
        "next": "14.1.0",
        "next-auth": "^4.24.5",
        "nextjs-progressbar": "^0.0.16",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "^7.45.2",
        "react-simple-typewriter": "^5.0.1",
        "server-only": "^0.0.1",
        "sharp": "^0.32.6",
        "superjson": "^1.13.1",
        "uuid": "^9.0.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@jest/globals": "^29.4.2",
        "@next/bundle-analyzer": "^13.4.13",
        "@next/eslint-plugin-next": "14.1.0",
        "@types/node": "18",
        "@types/react": "18.2.0",
        "@types/react-dom": "18.2.0",
        "@types/uuid": "^9.0.2",
        "dotenv": "^16.0.3",
        "eslint": "8.40.0",
        "eslint-config-next": "14.1.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jest": "^29.5.0",
        "node-fetch": "^3.3.1",
        "openapi-typescript": "^6.7.4",
        "prettier": "2.8.7",
        "prettier-plugin-organize-imports": "^3.2.4",
        "prisma": "5.9.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "tsafe": "^1.6.6",
        "typescript": "^5.3.3",
        "vercel": "latest"
    }
}
