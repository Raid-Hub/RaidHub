{
    "name": "raidhub",
    "version": "2023.7.17.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "dev+": "node proxy/server.js",
        "db:start": "docker-compose -f docker-compose.yml up -d",
        "db:seed": "prisma db seed",
        "db:schema": "prisma format && prisma db push",
        "db:push": "prisma format && prisma db push --force-reset && prisma db seed",
        "db:stop": "docker-compose -f docker-compose.yml down",
        "studio": "prisma studio",
        "analyze": "ANALYZE=true yarn build",
        "lint": "next lint",
        "test": "jest",
        "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\" && prisma format",
        "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css}\"",
        "sign": "bash ./proxy/sign.sh",
        "build": "bash ./vercel/build.sh",
        "deploy": "bash ./vercel/dev-deploy.sh"
    },
    "prisma": {
        "seed": "ts-node prisma/seed.ts"
    },
    "dependencies": {
        "@discordjs/collection": "^1.5.1",
        "@hookform/resolvers": "^3.1.1",
        "@next-auth/prisma-adapter": "^1.0.7",
        "@prisma/client": "^5.2.0",
        "@tanstack/react-query": "^4.32.6",
        "aws-sdk": "^2.1417",
        "bungie-net-core": "^2.1.0",
        "dexie": "^3.2.4",
        "formidable": "^3.5.0",
        "framer-motion": "^10.13.0",
        "html2canvas": "^1.4.1",
        "next": "^13.1.6",
        "next-auth": "^4.23.0",
        "nextjs-progressbar": "^0.0.16",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.45.2",
        "react-simple-typewriter": "^5.0.1",
        "sharp": "^0.32.4",
        "uuid": "^9.0.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@jest/globals": "^29.4.2",
        "@next/bundle-analyzer": "^13.4.13",
        "@types/cookies": "^0.7.7",
        "@types/formidable": "^3.4.0",
        "@types/node": "18.0.0",
        "@types/react": "^18.0.33",
        "@types/react-dom": "18.0.5",
        "@types/uuid": "^9.0.2",
        "dotenv": "^16.0.3",
        "eslint": "8.40.0",
        "eslint-config-next": "13.4.2",
        "eslint-plugin-unused-imports": "^2.0.0",
        "jest": "^29.5.0",
        "node-fetch": "^3.3.1",
        "prettier": "2.8.7",
        "prisma": "^5.2.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "<5.1.0",
        "vercel": "^31.2.2"
    }
}
